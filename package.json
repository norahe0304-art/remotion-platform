{
  "name": "remotion-platform",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*",
    "templates/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "check": "npm run check --workspaces --if-present",
    "prompts:list": "node scripts/prompts-list.mjs",
    "prompts:search": "node scripts/prompts-search.mjs",
    "prompts:sync": "node scripts/prompts-sync.mjs",
    "prompts:sync:since": "node scripts/prompts-sync.mjs --since",
    "prompts:app": "node scripts/prompts-app.mjs",
    "prompts:open": "node scripts/prompts-app.mjs --open"
  }
}
